{
  "name": "@leadlah/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @leadlah/core build && nest start --watch",
    "start": "node dist/main",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "ts-node -r tsconfig-paths/register src/database/migrate.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seed.ts"
  },
  "dependencies": {
    "@leadlah/core": "workspace:*",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.14",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.14",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.14",
    "@nestjs/typeorm": "^11.0.0",
    "bullmq": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "0.35.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.14",
    "@types/express": "^5.0.6",
    "@types/node": "^20.9.3",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "drizzle-kit": "0.26.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "sql.js": "^1.13.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^2.1.4"
  }
}
